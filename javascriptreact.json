	"React Functional Component": {
		"prefix": "rsc",
		"body": [
			"import React from 'react'",
			"",
			"export default function $TM_FILENAME_BASE() {",
			"   return <div>",
			"		$1",
			"   </div>",
			"}"
		],
		"description": "React Functional Component"
