	"React Class Component": {
		"prefix": "rcc",
		"body": [
			"import React, { Component } from 'react';",
			"",
			"class $TM_FILENAME_BASE extends Component {",
			"    constructor(props) {",
			"        super(props);",
			"        this.state = {",
			"            ${1:stateValue}: ${2:value}",
			"        };",
			"    }",
			"",
			"",
			"    render() {",
			"        return (",
			"            <div>",
			"                $3",
